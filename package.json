{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "migrate": "npx dotenv -e .env.local -- prisma migrate deploy",
    "build:production": "npx dotenv -e .env.local -- npm run migrate && npm run generate && npm run build"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.8",
    "@prisma/client": "^5.7.1",
    "init": "^0.1.2",
    "moment": "^2.30.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}